	for i,v in pairs(descendants) do
		if v.Name == "Water" then
			v:Destroy()

		end

		local Descendants = game:GetService("Workspace").WorldIgnore.MapEffects:GetDescendants()

		for i,v in pairs(Descendants) do
			if v.Name == "WaterPosition" then
				v:Destroy()

			end

		end
	end
end)


