-- Scripts:
function SCRIPT_CPEY88_FAKESCRIPT() -- Base.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Base
	base = Base
	
	base.Visible = not base.Visible
	base.Parent.Title.Visible = not base.Parent.Title.Visible
	ESPname = "FESPr"
	function newtEntry(n,v)
			b = game.Lighting:FindFirstChild(ESPname.."config"):FindFirstChild(n)
			if v == "t" then
				b.Value = not b.Value 
			else
				b.Value = v
			end
	end
	
	local function readVt(n)
			v = game.Lighting:FindFirstChild(ESPname.."config"):FindFirstChild(n).Value
			if typeof(v) == "boolean" then
				if v == false then
					return "OFF"
				else
					return "ON"
				end
			else
				return v
			end
			
	end
	
	--[[
		newEntry("BoolValue","FrogEyes",false)
		newEntry("BoolValue","FrogESP",true)
		newEntry("BoolValue","NPCesp",false)
		newEntry("StringValue","FHtarget","")
	]]--
	
	--functions
	--[[
	base["Frog Eyes"].Act.MouseButton1Click:connect(function()
	  	newEntry("FrogEyes","t")
		tmp = readV("FrogEyes")
		base["Frog Eyes"].State.Text = tmp
	end)
	]]--
	tmp = readVt("FrogESP")
		base["Frog ESP"].State.Text = tmp
	base["Frog ESP"].Act.MouseButton1Click:connect(function()
	  	newtEntry("FrogESP","t")
		tmp = readVt("FrogESP")
		base["Frog ESP"].State.Text = tmp
	end)
	
	tmp = readVt("FHtarget")
		if game:GetService("Players"):FindFirstChild(tmp) ~= nil then
			base["Frog Harass"].ImageLabel.Image = "http://www.roblox.com/thumbs/avatar.ashx?x=352&y=352&format=png&username="..tmp
			base["Frog Harass"].Target.Text = "Targeting "..tmp
		else
			base["Frog Harass"].ImageLabel.Image = ""
			base["Frog Harass"].Target.Text = "Player Not Found"
		end
	base["Frog Harass"].Act.MouseButton1Click:connect(function()
	  	newtEntry("FHtarget",base["Frog Harass"].TextBox.Text)
		tmp = readVt("FHtarget")
		if game:GetService("Players"):FindFirstChild(tmp) ~= nil then
			base["Frog Harass"].ImageLabel.Image = "http://www.roblox.com/thumbs/avatar.ashx?x=352&y=352&format=png&username="..tmp
			base["Frog Harass"].Target.Text = "Targeting "..tmp
		else
			base["Frog Harass"].ImageLabel.Image = ""
			base["Frog Harass"].Target.Text = "Player Not Found"
		end
	end)
	
	tmp = readVt("NPCesp")
		base["NPC ESP"].State.Text = tmp
	base["NPC ESP"].Act.MouseButton1Click:connect(function()
	  	newtEntry("NPCesp","t")
		tmp = readVt("NPCesp")
		base["NPC ESP"].State.Text = tmp
		pcnpc = false
	end)
	--button
	

end
coroutine.resume(coroutine.create(SCRIPT_CPEY88_FAKESCRIPT))

end

--ACTUAL SCRIPT
genBilUI()
local UIS = game:GetService("UserInputService")
	
	UIS.InputBegan:connect(function(input)
		if input.KeyCode == Enum.KeyCode.F4 then
			tt = game:GetService("Players").LocalPlayer.PlayerGui[ESPname.."PLR"].Base
			tt.Visible = not tt.Visible
			tt.Parent.Title.Visible = not tt.Parent.Title.Visible
		end
	end)

function Notify(title,text,duration,icon,func,BTN,BTN2)
	
		game.StarterGui:SetCore("SendNotification", {Title=title,Text=text,Icon=icon,Duration=duration,Callback=func,Button1=BTN,Button2=BTN2})
	end
	
	Notify("FROG ESP v3","F4 to open UI",5,"rbxassetid://900690309")

ESPname = "FESPr"



	--modules
	local FrogEyes
	local FrogESP
	local NPCesp
	--settings
	local FHtarget
	--end

pcnpc = true

function newEntry(t,n,v)
		b = Instance.new(t)
		b.Name = n
		b.Value = v 
		b.Parent = game.Lighting:FindFirstChild(ESPname.."config")
end

--make config
if game.Lighting:FindFirstChild(ESPname.."config") == nil then
	con = Instance.new("Configuration")
	con.Name = ESPname .. "config"
	con.Parent = game.Lighting
	
	newEntry("BoolValue","FrogEyes",false)
	newEntry("BoolValue","FrogESP",true)
	newEntry("BoolValue","NPCesp",false)
	newEntry("StringValue","FHtarget","")
end


con = game.Lighting:FindFirstChild(ESPname.."config")


while true do
	function NPC()
			for index, child in pairs(game.Workspace:GetDescendants()) do
			 	--print(index, child.Name .. " -[".. child.ClassName .."]")
				--check if it exists in the workspace
				if child.ClassName == "Humanoid" then
					if game:GetService("Players"):FindFirstChild(child.Parent.name) == nil then
						--check if there is not already an ESP on it
						plr = child.Parent
						if plr:FindFirstChild("Head"):FindFirstChild(ESPname) == nil then
							if NPCesp then
							
							--if not then add one
								tmp = ESP:Clone()
								tmp.Parent = plr.Head
							end
						else
							--if there is try to update it
							--if off take it away
							if plr:FindFirstChild("Head") ~= nil then
								if NPCesp == false then
									plr.Head:FindFirstChild(ESPname):Destroy()
								else
									--team [X], name [X], HP [X] 
										e = plr.Head:FindFirstChild(ESPname)
										e.Frame.BackgroundColor3 = Color3.new(0.9,0.8,0.2)
										e.Frame.BorderColor3 = Color3.new(1,1,1)
										e.HP.Text = math.floor(child.Health) .. "/" .. math.floor(child.MaxHealth)
										e.nme.Text = tostring(child.Parent.Name)
										e.Type.Text = "NPC"
						
								end
							end
						end
					end
				end
				pcnpc = true
			end
	end

	function plrE()
		ESP = game.Lighting:FindFirstChild(ESPname)
		dir = game:GetService("Players")
		--player ESP
		for i, child in pairs(dir:GetChildren()) do
			--check if it exists in the workspace 
			if child.Character and child.Name ~= game:GetService("Players").LocalPlayer.Name then
				--check if there is not already an ESP on it
				plr = child.Character
				if plr ~= nil and plr:FindFirstChild(ESPname) == nil then
					if FrogESP then
					--if not then add one AVOID ADDING TO PLAYER
						tmp = ESP:Clone()
						tmp.Parent = plr
					end
				else
					--if there is try to update it
					--if off take it away
					if FrogESP == false and plr ~= nil then
						plr:FindFirstChild(ESPname):Destroy()
					else
						--team [X], name [X], HP [X] 
						if plr ~= nil then
							e = plr:FindFirstChild(ESPname)
							e.Frame.BackgroundColor3 = game:GetService("Players")[plr.Name].TeamColor.Color
							e.HP.Text = math.floor(plr.Humanoid.Health) .. "/" .. math.floor(plr.Humanoid.MaxHealth)
							e.nme.Text = tostring(child.name)
							e.Type.Text = "Player"
							e.Type.TextColor3 = Color3.fromRGB(255, 255, 0)
							e.Type.TextStrokeColor3 = Color3.fromRGB(112, 112, 0)
							
							if con.FHtarget.Value == child.name then
								e.Frame.BorderColor3 = Color3.new(1,0,0)
								e.Type.Text = "TARGET"
								e.Type.TextColor3 = Color3.fromRGB(255, 76, 0)
								e.Type.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
							end
						end
					end
				end
			end
		end
	end
	
	if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild(ESPname.."PLR") == nil then
		genPlrGUI()
	end
	wait(0.1)
	if NPCesp == true then
		spawn(NPC)
	elseif pcnpc == false then
		spawn(NPC)
	end
	
	spawn(plrE)
	--update Vars
	con = game.Lighting:FindFirstChild(ESPname.."config")
	local function readV(n)
		
		return game.Lighting:FindFirstChild(ESPname.."config")[n].Value
	end
	
	--modules
	FrogEyes = readV("FrogEyes")
	FrogESP = readV("FrogESP")
	NPCesp = readV("NPCesp")
	--settings
	FHtarget = readV("FHtarget")

	--end

	
	
	
	if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild(ESPname.."PLR") == nil then
		genPlrGUI()
	end
		
end
end)

local e = ESP:Button("Box ESP 2", function()
----box esp


local Holder = Instance.new("Folder", game.CoreGui)
Holder.Name = "ESP"

local UpdateFuncs = {}

local Box = Instance.new("BoxHandleAdornment")
Box.Name = "nilBox"
Box.Size = Vector3.new(4, 7, 4)
Box.Color3 = Color3.new(100 / 255, 100 / 255, 100 / 255)
Box.Transparency = 0.7
Box.ZIndex = 0
Box.AlwaysOnTop = true
Box.Visible = true

local NameTag = Instance.new("BillboardGui")
NameTag.Name = "nilNameTag"
NameTag.Enabled = false
NameTag.Size = UDim2.new(0, 200, 0, 50)
NameTag.AlwaysOnTop = true
NameTag.StudsOffset = Vector3.new(0, 1.8, 0)
local Tag = Instance.new("TextLabel", NameTag)
Tag.Name = "Tag"
Tag.BackgroundTransparency = 1
Tag.Position = UDim2.new(0, -50, 0, 0)
Tag.Size = UDim2.new(0, 300, 0, 20)
Tag.TextSize = 20
Tag.TextColor3 = Color3.new(100 / 255, 100 / 255, 100 / 255)
Tag.TextStrokeColor3 = Color3.new(0 / 255, 0 / 255, 0 / 255)
Tag.TextStrokeTransparency = 0.4
Tag.Text = "nil"
Tag.Font = Enum.Font.SourceSansBold
Tag.TextScaled = false

local LoadCharacter = function(v)
	repeat wait() until v.Character ~= nil
	v.Character:WaitForChild("Humanoid")
	local vHolder = Holder:FindFirstChild(v.Name)
	vHolder:ClearAllChildren()
	local b = Box:Clone()
	b.Name = v.Name .. "Box"
	b.Adornee = v.Character
	b.Parent = vHolder
	local t = NameTag:Clone()
	t.Name = v.Name .. "NameTag"
	t.Enabled = true
	t.Parent = vHolder
	t.Adornee = v.Character:WaitForChild("Head", 5)
	if not t.Adornee then
		return UnloadCharacter(v)
	end
	t.Tag.Text = v.Name
	b.Color3 = Color3.new(v.TeamColor.r, v.TeamColor.g, v.TeamColor.b)
	t.Tag.TextColor3 = Color3.new(v.TeamColor.r, v.TeamColor.g, v.TeamColor.b)
	local UpdateNameTag = function()
		if not pcall(function()
			--v.Character.Humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
			local maxh = math.floor(v.Character.Humanoid.MaxHealth)
			local h = math.floor(v.Character.Humanoid.Health)
			t.Tag.Text = v.Name .. "\n" .. ((maxh ~= 0 and tostring(math.floor((h / maxh) * 100))) or "0") .. "%  " .. tostring(h) .. "/" .. tostring(maxh)
		end) then
			UpdateFuncs[v] = nil
		end
	end
	UpdateNameTag()
	UpdateFuncs[v] = UpdateNameTag
end

local UnloadCharacter = function(v)
	local vHolder = Holder:FindFirstChild(v.Name)
	if vHolder and (vHolder:FindFirstChild(v.Name .. "Box") ~= nil or vHolder:FindFirstChild(v.Name .. "NameTag") ~= nil) then
		vHolder:ClearAllChildren()
	end
end

local LoadPlayer = function(v)
	local vHolder = Instance.new("Folder", Holder)
	vHolder.Name = v.Name
	v.CharacterAdded:Connect(function()
		pcall(LoadCharacter, v)
	end)
	v.CharacterRemoving:Connect(function()
		pcall(UnloadCharacter, v)
	end)
	LoadCharacter(v)
end

local UnloadPlayer = function(v)
	UnloadCharacter(v)
	local vHolder = Holder:FindFirstChild(v.Name)
	if vHolder then
		vHolder:Destroy()
	end
end

for i,v in pairs(game:GetService("Players"):GetPlayers()) do
	spawn(function() pcall(LoadPlayer, v) end)
end

game:GetService("Players").PlayerAdded:Connect(function(v)
	pcall(LoadPlayer, v)
end)

game:GetService("Players").PlayerRemoving:Connect(function(v)
	pcall(UnloadPlayer, v)
end)

game.ItemChanged:Connect(function(i, v)
	if i:IsA("Player") and v == "TeamColor" then
		if Holder:FindFirstChild(i.Name) then
			UnloadCharacter(i)
			wait()
			LoadCharacter(i)
		end
	elseif i:IsA("Humanoid") and i.Parent then
		local p = game:GetService("Players"):GetPlayerFromCharacter(i.Parent)
		if p and Holder:FindFirstChild(p.Name) then
			UpdateFuncs[p]()
		end
	end
end)

game:GetService("Players").LocalPlayer.NameDisplayDistance = 0
game:GetService("Players").LocalPlayer.HealthDisplayDistance = 0
end)

